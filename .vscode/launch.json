	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Program with Input",
			"type": "node",
			"request": "launch",
			"program": "${workspaceFolder}/index.js",
			"console": "integratedTerminal"
		},
		{
			"name": "Launch Program with Predefined Input",
			"type": "node",
			"request": "launch",
			"program": "${workspaceFolder}/index.js",
			"args": [
				"array ( ... )"
			],
			"console": "integratedTerminal"
		}
	]
}
